/*使屏幕变暗  */

.commodity_screen {
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	background: #000;
	opacity: 0.2;
	overflow: hidden;
	z-index: 998;
	color: #fff;
}

.commodity_attr_box {
	z-index: 999;
	position: fixed;
	bottom: 0;
	left: 0;
	right: 0;
	background: #fff;
}

.commodity_attr_box button::after {
	border: none;
}

.commodity_attr_box button {
	background-color: #fff;
	color: #555555;
}

.share_tit {
	height: 30px;
	line-height: 30px;
	background: #f4f4f4;
	display: flex;
	justify-content: center;
	align-items: center;
	justify-content: space-between;
}

.share_tit .share_close {
	display: block;
	padding: 0px 20px;
}

.share_tit .share {
	display: block;
	padding-left: 10px;
}

.close {
	position: absolute;
	top: -24rpx;
	right: -24rpx;
	font-size: 16px;
	background: #ffffff;
	border: 1px solid #ccc;
	border-radius: 50%;
	padding: 0rpx 8rpx;
}

.share_box {
	display: flex;
	justify-content: center;
	align-items: center;
	padding: 50rpx 0;
}

.sharebtn {
	flex: 1;
	border-right: #E7E7E7 1rpx solid;
}

.sharebtn:last-child {
	border-right: none;
}

.sharebtn-wx {
	background-image: url("data:image/jpeg;base64,");
	background-size: auto 100rpx;
	background-repeat: no-repeat;
	background-position: center top;
	height: 170rpx;
	line-height: 50rpx;
	padding-top: 120rpx;
	font-size: 28rpx;
}

.sharebtn-pyq {
	background-image: url("data:image/jpeg;base64,");
	background-size: auto 100rpx;
	background-repeat: no-repeat;
	background-position: center top;
	height: 170rpx;
	line-height: 50rpx;
	padding-top: 120rpx;
	font-size: 28rpx;
}


