/* 规格弹层 */

.attr-pop {
	z-index: 10;
	position: fixed;
	left: 0;
	right: 0;
	bottom: 110rpx;
	background: #fff;
	border-top-left-radius: 10rpx;
	border-top-right-radius: 10rpx;
}

.attr-pop .img-info {
	padding: 28rpx;
	overflow: hidden;
	border-bottom: #ECECEC 1rpx solid;
	display: flex;
}

.img-info .price {
	margin: 20rpx 28rpx;
	color: #BD3543;
	display: flex;
}

.img-info .price .selling {
	color: #BD3543;
	font-size: 42rpx;
	line-height: 60rpx;
}

.img-info .price .market {
	color: #7A7A7A;
	font-size: 26rpx;
	line-height: 60rpx;
	margin-left: 20rpx;
}


.attr-pop .img {
	height: 157rpx;
	width: 157rpx;
	background: #f4f4f4;
	margin-right: 20rpx;
}

.attr-pop .info {
	flex: 1;
}

.attr-pop .p {
	font-size: 36rpx;
	color: #BD3543;
	line-height: 40rpx;
	padding: 10rpx 0;
}

.attr-pop .d {
	font-size: 26rpx;
	color: #7A7A7A;
	line-height: 30rpx;
	padding-bottom: 15rpx;
}

.attr-pop .a {
	font-size: 30rpx;
	color: #2D2D2D;
	line-height: 40rpx;
	overflow: hidden;
}

.attr-pop .a .aa {
	float: left;
}

