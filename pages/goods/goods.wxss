.goods-frame {
	height: calc(100% - 110rpx);
}
.goods-box {
	height: 100%;
	background: #F4F4F4;
}

.detail {
	width: 750rpx;
	height: auto;
	overflow: hidden;
}

.detail image {
	width: 750rpx;
	display: block;
}


/* 底部按钮 */

.bottom-btn {
	position: fixed;
	left: 0;
	right: 0;
	bottom: 0;
	height: 110rpx;
	z-index: 8;
	display: flex;
	background: #fff;
	box-shadow: 0 0 12rpx rgba(0, 0, 0, 0.25);
}
.bottom-btn.imp-bottom-btn {
	z-index: 10;
}

.bottom-btn .btns {
	flex: 1;
	display: flex;
	align-items: center;
	justify-content: center;
}

.bottom-btn .btns .btn {
	flex: 1;
	text-align: center;
	color: #7A7A7A;
	font-size: 20rpx;
	line-height: 28rpx;
	padding: 0;
	background: none;
	position: relative;
}

.bottom-btn .btns .btn .img {
	width: 48rpx;
	height: 48rpx;
	margin: 0 auto;
}

.bottom-btn .btns .btn .cart-count {
	position: absolute;
	top: 0;
	right: 0;
	line-height: 30rpx;
	font-size: 20rpx;
	background: #FF0000;
	border-radius: 100rpx;
	min-width: 30rpx;
	color: #FFF;
}

.add-cart {
	width: 214rpx;
	line-height: 110rpx;
	color: #2D2D2D;
	font-size: 32rpx;
	text-align: center;
}

.now-buy {
	width: 214rpx;
	line-height: 110rpx;
	background: #BD3543;
	color: #FFFFFF;
	font-size: 32rpx;
	text-align: center;
}
.now-buy.disabled {
	/*background: #CCC;*/
}

