.container {
	/* margin-bottom: 100rpx; */
}

.goodsimgs {
	width: 750rpx;
	height: 750rpx;
}

.goodsimgs image {
	width: 750rpx;
	height: 750rpx;
}

.goods-info {
	background: #fff;
	margin-bottom: 20rpx;
}

.goods-info .name-box {
	padding: 20rpx 0 0 28rpx;
	display: flex;
	align-items: center;
}

.goods-info .name {
	flex: 1;
	height: 96rpx;
	font-size: 34rpx;
	line-height: 48rpx;
	font-weight: 600;
}

.goods-info .sharegood-btn {
	width: 150rpx;
	height: 80rpx;
	line-height: 30rpx;
	text-align: center;
	color: #998A6E;
	font-size: 24rpx;
	border-left: #E1E1E1 1rpx solid;
	margin-left: 20rpx;
}

.goods-info .sharegood-btn image {
	width: 40rpx;
	height: 40rpx;
	margin: 0 auto 10rpx;
}

.goods-info .price {
	margin: 20rpx 28rpx;
	color: #BD3543;
	display: flex;
}

.goods-info .price .selling {
	color: #BD3543;
	font-size: 42rpx;
	line-height: 60rpx;
}

.goods-info .price .market {
	color: #7A7A7A;
	font-size: 26rpx;
	line-height: 60rpx;
	margin-left: 20rpx;
}

.goods-info .sales {
	margin: 20rpx 28rpx;
	height: 42rpx;
	color: #BD3543;
	display: flex;
	justify-content: space-between;
}

.goods-info .sales .express-delivery {
	color: #7A7A7A;
	font-size: 26rpx;
	line-height: 42rpx;
}

.goods-info .sales .sold {
	color: #7A7A7A;
	font-size: 26rpx;
	line-height: 42rpx;
	margin-left: 20rpx;
}

.goods-info .share-btn {
	position: absolute;
	right: 31.25rpx;
	top: 38rpx;
	z-index: 11;
}

.goodsPosters {
	width: 0;
	height: 0;
	overflow: visible;
}

.goodsPosters canvas {
	display: none;
}

.goodsPostersShow {
	position: fixed;
	left: 75rpx;
	top: 75rpx;
	z-index: 999;
	width: 600rpx;
	height: 1000rpx;
	background: #FFF;
	border-radius: 5px;
	box-shadow: 0 0 10px rgba(0, 0, 0, 0.5);
}

.goodsPostersShow canvas {
	display: block !important;
	width: 560rpx;
	height: 860rpx;
	margin: 20rpx;
}

.goodsPostersShow .btns {
	clear: both;
	margin: 20rpx;
	background: #0D8DDB;
	color: #FFF;
	text-align: center;
	line-height: 80rpx;
	border-radius: 5px;
}

.goodsPostersMask {
	z-index: 998;
	position: fixed;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	background: rgba(0, 0, 0, 0.5);
}

/* 商品规格 */
.section-nav {
	height: 100rpx;
	padding: 0 28rpx;
	background: #fff;
	margin-bottom: 20rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
}

.section-nav .t {
	width: 200rpx;
	font-size: 30rpx;
	color: #333;
	color: #2D2D2D;
}

.section-nav .specification {
	flex: 1;
	color: #998A6E;
	font-size: 26rpx;
	text-align: right;
}

.section-nav .i {
	color: #998A6E;
	width: 28rpx;
	height: 13rpx;
	margin-left: 20rpx;
}
.section-nav .i image {
	width: 100%;
	height: 100%;
}

.section-act .t {
	float: left;
	display: flex;
	align-items: center;
	width: 600rpx;
	height: 108rpx;
	overflow: hidden;
	line-height: 108rpx;
	font-size: 29rpx;
	color: #999;
	margin-left: 31.25rpx;
}

.section-act .label {
	color: #999;
}

.section-act .tag {
	display: flex;
	align-items: center;
	padding: 0 10rpx;
	border-radius: 3px;
	height: 37rpx;
	width: auto;
	color: #f48f18;
	overflow: hidden;
	border: 1px solid #f48f18;
	font-size: 25rpx;
	margin: 0 10rpx;
}

.section-act .text {
	display: flex;
	align-items: center;
	height: 37rpx;
	width: auto;
	overflow: hidden;
	color: #f48f18;
	font-size: 29rpx;
}

.comments {
	width: 100%;
	height: auto;
	padding-left: 30rpx;
	background: #fff;
	margin: 20rpx 0;
}

.comments .h {
	height: 102.5rpx;
	line-height: 100.5rpx;
	width: 718.75rpx;
	padding-right: 16rpx;
	border-bottom: 1px solid #d9d9d9;
}

.comments .h .t {
	display: block;
	float: left;
	width: 50%;
	font-size: 38.5rpx;
	color: #333;
}

.comments .h .i {
	display: block;
	float: right;
	width: 164rpx;
	height: 100.5rpx;
	line-height: 100.5rpx;
	background: url(http://nos.netease.com/mailpub/hxm/yanxuan-wap/p/20150730/style/img/icon-normal/address-right-990628faa7.png) right center no-repeat;
	background-size: 52rpx;
}

.comments .b {
	height: auto;
	width: 720rpx;
}

.comments .item {
	height: auto;
	width: 720rpx;
	overflow: hidden;
}

.comments .info {
	height: 127rpx;
	width: 100%;
	padding: 33rpx 0 27rpx 0;
}

.comments .user {
	float: left;
	width: auto;
	height: 67rpx;
	line-height: 67rpx;
	font-size: 0;
}

.comments .user image {
	float: left;
	width: 67rpx;
	height: 67rpx;
	margin-right: 17rpx;
	border-radius: 50%;
}

.comments .user text {
	display: inline-block;
	width: auto;
	height: 66rpx;
	overflow: hidden;
	font-size: 29rpx;
	line-height: 66rpx;
}

.comments .time {
	display: block;
	float: right;
	width: auto;
	height: 67rpx;
	line-height: 67rpx;
	color: #7f7f7f;
	font-size: 25rpx;
	margin-right: 30rpx;
}

.comments .content {
	width: 720rpx;
	padding-right: 30rpx;
	line-height: 45.8rpx;
	font-size: 29rpx;
	margin-bottom: 24rpx;
}

.comments .imgs {
	width: 720rpx;
	height: auto;
	margin-bottom: 25rpx;
}

.comments .imgs .img {
	height: 150rpx;
	width: 150rpx;
	margin-right: 28rpx;
}

.comments .spec {
	width: 720rpx;
	padding-right: 30rpx;
	line-height: 30rpx;
	font-size: 24rpx;
	color: #999;
	margin-bottom: 30rpx;
}

