/* 隐藏难看的滚动条 */
::-webkit-scrollbar {
	width: 0;
	height: 0;
	color: transparent;
}

page {
	height: 100%;
	overflow: hidden;
}

.container {
}

.goodsimgs {
	width: 750rpx;
	height: 750rpx;
}

.goodsimgs image {
	width: 750rpx;
	height: 750rpx;
}

.goods-info {
	background: #fff;
	margin-bottom: 20rpx;
}

.goods-info .name-box {
	padding: 20rpx 0 0 28rpx;
	display: flex;
	align-items: center;
}

.goods-info .name {
	flex: 1;
	height: 96rpx;
	font-size: 34rpx;
	line-height: 48rpx;
	font-weight: 600;
}

.goods-info .sharegood-btn {
	width: 150rpx;
	height: 80rpx;
	line-height: 30rpx;
	text-align: center;
	color: #998A6E;
	font-size: 24rpx;
	border-left: #E1E1E1 1rpx solid;
	margin-left: 20rpx;
}

.goods-info .sharegood-btn image {
	width: 40rpx;
	height: 40rpx;
	margin: 0 auto 10rpx;
}

.goods-info .price {
	margin: 20rpx 28rpx;
	color: #BD3543;
	display: flex;
}

.goods-info .price .selling {
	color: #BD3543;
	font-size: 42rpx;
	line-height: 60rpx;
}

.goods-info .price .market {
	color: #7A7A7A;
	font-size: 26rpx;
	line-height: 60rpx;
	margin-left: 20rpx;
}

.goods-info .sales {
	margin: 20rpx 28rpx;
	height: 42rpx;
	color: #BD3543;
	display: flex;
	justify-content: space-between;
}

.goods-info .sales .express-delivery {
	color: #7A7A7A;
	font-size: 26rpx;
	line-height: 42rpx;
}

.goods-info .sales .sold {
	color: #7A7A7A;
	font-size: 26rpx;
	line-height: 42rpx;
	margin-left: 20rpx;
}

.goods-info .share-btn {
	position: absolute;
	right: 31.25rpx;
	top: 38rpx;
	z-index: 11;
}

.goodsPosters {
	width: 0;
	height: 0;
	overflow: visible;
}

.goodsPosters canvas {
	display: none;
}

.goodsPostersShow {
	z-index: 99;
	position: absolute;
	left: 50%;
	top: 150rpx;
	width: 538rpx;
	transform: translate(-50%, -50%);
}

.goodsPostersShow canvas {
	display: block !important;
	width: 538rpx;
	height: 830rpx;
	border-radius: 6rpx;
	overflow: hidden;
}

.goodsPostersShow .btns {
	clear: both;
	margin: 28rpx 0 15rpx 0;
	background: #BD3543;
	color: #FFF;
	font-size: 32rpx;
	text-align: center;
	line-height: 100rpx;
	border-radius: 6px;
}
.goodsPostersShow .desc {
	font-size: 26rpx;
	line-height: 36rpx;
	color: #FFFFFF;
	text-align: center;
}

.mask-bg {
	z-index: 9;
	position: fixed;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	background: rgba(0, 0, 0, 0.6);
}

/* 商品规格 */

.section-nav {
	height: 100rpx;
	padding: 0 28rpx;
	background: #fff;
	margin-bottom: 20rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
}

.section-nav .t {
	width: 200rpx;
	font-size: 30rpx;
	color: #333;
	color: #2D2D2D;
}

.section-nav .specification {
	flex: 1;
	color: #998A6E;
	font-size: 26rpx;
	text-align: right;
}

.section-nav .i {
	color: #998A6E;
	width: 30rpx;
	height: 30rpx;
	margin-left: 20rpx;
}

.section-nav .i image {
	width: 100%;
	height: 100%;
}

.section-act .t {
	float: left;
	display: flex;
	align-items: center;
	width: 600rpx;
	height: 108rpx;
	overflow: hidden;
	line-height: 108rpx;
	font-size: 29rpx;
	color: #999;
	margin-left: 31.25rpx;
}

.section-act .label {
	color: #999;
}

.section-act .tag {
	display: flex;
	align-items: center;
	padding: 0 10rpx;
	border-radius: 3px;
	height: 37rpx;
	width: auto;
	color: #f48f18;
	overflow: hidden;
	border: 1px solid #f48f18;
	font-size: 25rpx;
	margin: 0 10rpx;
}

.section-act .text {
	display: flex;
	align-items: center;
	height: 37rpx;
	width: auto;
	overflow: hidden;
	color: #f48f18;
	font-size: 29rpx;
}

.goods-attr {
	padding: 0 28rpx;
	background: #FAFAFA;
}

.goods-attr .t {
	color: #2D2D2D;
	line-height: 100rpx;
	font-size: 30rpx;
}


