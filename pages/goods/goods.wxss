.member-ad {
	background-color: #fff;
	background-image: url("https://baifen-app.oss-cn-beijing.aliyuncs.com/weixin/images/member-ad-bg.png");
	background-size: 100% 100%;
	background-repeat: repeat-x;
	height: 100rpx;
	display: flex;
	align-items: center;
	padding: 0 28rpx;
}

.member-ad .kt-icon {
	width: 54rpx;
	height: 49rpx;
	margin-right: 20rpx;
}

.member-ad .kt-icon image {
	width: 100%;
	height: 100%;
}

.member-ad .kt-text {
	color: #D5AE87;
}

.member-ad .kt-btn-box {
	flex: 1;
}

.member-ad .kt-btn-box .kt-btn {
	font-size: 24rpx;
