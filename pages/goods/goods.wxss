}

.attr-pop .img-info {
	width: 687.5rpx;
	height: 177rpx;
	overflow: hidden;
	margin-bottom: 41.5rpx;
}

.attr-pop .img {
	float: left;
	height: 177rpx;
	width: 177rpx;
	background: #f4f4f4;
