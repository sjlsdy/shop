/*对话框 */


/*使屏幕变暗  */
.share_frame {
	z-index: 11;
	position: fixed;
	left: 0;
	right: 0;
	bottom: 0;
	background: #FFFFFF;
}
.share_tit {
	height: 30px;
	padding: 50rpx;
}
.share_tit .share {
	position: relative;
	text-align: center;
}

.share_tit .share::before {
	content: "";
	border-top: #E1E1E1 1rpx solid;
	width: 100rpx;
	height: 30rpx;
	position: absolute;
	top: 15rpx;
	left: 150rpx;
}
.share_tit .share::after {
	content: "";
	border-top: #E1E1E1 1rpx solid;
	width: 100rpx;
	height: 30rpx;
	position: absolute;
	top: 15rpx;
	right: 150rpx;
}

.close {
	background-image: url("https://baifen-app.oss-cn-beijing.aliyuncs.com/weixin/images/icon-close-btn-white.png");
	background-size: cover;
	width: 60rpx;
	height: 60rpx;
	position: absolute;
	top: -80rpx;
	right: -80rpx;
}

.share_box {
	display: flex;
	justify-content: center;
	align-items: center;
	padding: 50rpx 80rpx;
}

.sharebtn {
	flex: 1;
}

.sharebtn-wx {
	background-color: #FFF;
	background-image: url("https://baifen-app.oss-cn-beijing.aliyuncs.com/weixin/images/icon-share-wx.png");
	background-size: auto 109rpx;
	background-repeat: no-repeat;
	background-position: center top;
	height: 159rpx;
	line-height: 30rpx;
	padding-top: 129rpx;
	font-size: 26rpx;
	color: #7A7A7A;
}

.sharebtn-pyq {
	background-color: #FFF;
	background-image: url("https://baifen-app.oss-cn-beijing.aliyuncs.com/weixin/images/icon-share-friends.png");
	background-size: auto 109rpx;
	background-repeat: no-repeat;
	background-position: center top;
	height: 159rpx;
	line-height: 30rpx;
	padding-top: 129rpx;
	font-size: 26rpx;
	color: #7A7A7A;
}


