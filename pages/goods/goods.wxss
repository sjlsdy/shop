.spec-con {
	padding: 0 28rpx;
	overflow: hidden;
}

.spec-con .spec-item {}

.spec-con .spec-item .name {
	line-height: 80rpx;
	font-size: 26rpx;
	color: #2D2D2D;
}

.spec-con .values {
	display: flex;
	flex-wrap: wrap;
}

.spec-con .value {
	padding: 0 35rpx;
	line-height: 60rpx;
	text-align: center;
	margin: 0 20rpx 20rpx 0;
	border: 1px solid #E1E1E1;
	font-size: 28rpx;
	color: #7A7A7A;
	border-radius: 5rpx;
}

.spec-con .value.disable {
	border: 1rpx solid #BD3543;
	color: #BD3543;
}

.spec-con .value.selected {
	border: 1px solid #b4282d;
	color: #b4282d;
}

.number-item .selnum {
	width: 265rpx;
	height: 80rpx;
	border: 2rpx solid #C9C9C9;
	border-radius: 5rpx;
	overflow: hidden;
	display: flex;
}

.number-item .cut {
	width: 80rpx;
	height: 78rpx;
	line-height: 78rpx;
	text-align: center;
	color: #2D2D2D;
	border-right: 1px solid #C9C9C9;
}

.number-item .add {
	width: 80rpx;
	height: 78rpx;
	line-height: 78rpx;
	text-align: center;
	color: #2D2D2D;
	border-left: 1px solid #C9C9C9;
}

.number-item .number {
	flex: 1;
	text-align: center;
	line-height: 78rpx;
	height: 78rpx;
}


