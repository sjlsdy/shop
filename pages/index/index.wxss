/* 承诺 */
.home-committed-frame {
	display: flex;
	background: #FFF;
	padding: 20rpx 28rpx 20rpx;
}
.home-committed-frame .home-committed-item {
	margin-right: 28rpx;
	padding-left: 40rpx;
	font-size: 24rpx;
	color: #998a6e;
	background-image: url("https://baifen-app.oss-cn-beijing.aliyuncs.com/weixin/images/index-icon-slogan.png");
	background-size: auto 100%;
	background-repeat: no-repeat;
}

/* 今日推荐 */
.today-topic-frame {
	margin: 12rpx 0;
	background: #FFF;
	overflow: hidden;
}
.today-topic-title {
	line-height: 45rpx;
	margin: 20rpx 0;
	text-align: center;
	font-size: 32rpx;
}
.today-topic-box {
	display: flex;
}
.today-topic-item {
	flex-shrink:0;
	width: 250rpx;
	overflow: hidden;
	border-left: #E7E7E7 1rpx solid;
}
.today-topic-item:first-child {
	border-left: none;
}
.today-topic-item-box {
	width: 193rpx;
	margin: 0 auto;
}
.today-topic-item-image {
	width: 193rpx;
	height: 193rpx;
	margin: 0 auto;
}
.today-topic-item-image image {
	width: 193rpx;
	height: 193rpx;
}
.today-topic-item-name {
	width: 6em;
	height: 40rpx;
	line-height: 40rpx;
	overflow: hidden;
}
.today-topic-item-price {
	font-size: 28rpx;
	color: #952934;
}

/* 卖型分类 */
.goodtype-frame {
	display: flex;
	flex-wrap: wrap;
}
.goodtype-item {
	flex-shrink:0;
	width: 375rpx;
	display: flex;
	background: #FFF;
}
.goodtype-item:nth-child(n+3) {
	border-top: #ececec 2rpx solid;
}
.goodtype-item:nth-child(odd) {
	border-right: #ececec 2rpx solid;
}
.goodtype-item:nth-child(even) {
}
.goodtype-item:nth-child(1) .goodtype-text-title {
	color: #994d54;
}
.goodtype-item:nth-child(2) .goodtype-text-title {
	color: #6b8bac;
}
.goodtype-item:nth-child(3) .goodtype-text-title {
	color: #6c4f7a;
}
.goodtype-item:nth-child(4) .goodtype-text-title {
	color: #a58a60;
}
.goodtype-text {
	margin:30rpx 0 0 30rpx;
	position: relative;
}
.goodtype-text-title {
	font-size: 32rpx;
	line-height: 45rpx;
}
.goodtype-text-des {
	color: #727272;
	font-size: 24rpx;
}
.goodtype-img {
	flex-shrink:0;
	width: 163rpx;
	height: 163rpx;
	margin-top: 75rpx;
}
.goodtype-img image {
	width: 100%;
	height: 100%;
}
.goodtype-icon-forb6 {
	position: absolute;
	left: 258rpx;
	top: -30rpx;
	width: 59rpx;
	height: 63rpx;
}
.goodtype-icon-forb6 image {
	width: 100%;
	height: 100%;
}
/* 商品滑动  */
.goodsswiper__box {
	background: #FFF;
	margin: 12rpx 0 0 0;
}
.goodsswiper__header {
	border-bottom: 2rpx solid #f4f4f4;
}

.goodsswiper__tab {
	display: flex;
}

.goodsswiper__tab__item {
	flex-shrink:0;
	padding: 0 15rpx;
	line-height: 94rpx;
	text-align: center;
}

.goodsswiper__tab__item.active {
	color: #952934;
	position: relative;
}

.goodsswiper__tab__item.active::before {
	position: absolute;
	bottom: 20rpx;
	left: 50%;
	content: "";
	background: #952934;
	width: 60%;
	height: 3rpx;
	overflow: hidden;
	transform: translateX(-50%);
}

.goodsswiper__tab__content {
	display: flex;
	flex-wrap: wrap;
	background: #f4f4f4;
}

.good-item {
	width: 369rpx;
	height: 545rpx;
	background: #FFF;
	position: relative;
}
.good-item:nth-child(odd) {
	margin-right: 12rpx;
}
.good-item:nth-child(n+3) {
	margin-top: 12rpx;
}

.good-item-img {
	width:　369rpx;
	height:　369rpx;
	background: #FFF;
}
.good-item .good-item-text {
	padding: 28rpx
}
.good-item-img image {
	display: block;
}
.good-item-name {
	height: 80rpx;
	overflow: hidden;
}
.good-item-price {
	color: #952934;
	line-height: 40rpx;
}
.good-item .good-item-icon-forb6 {
	position: absolute;
	left: 290rpx;
	top: 0;
	width: 59rpx;
	height: 63rpx;
}
.good-item .good-item-icon-forb6 image {
	width: 100%;
	height: 100%;
}
